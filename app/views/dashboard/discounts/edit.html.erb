<%= form_for [@merchant, @discount], :as => :discount, :url => dashboard_discount_path(@discount) do |f| %>
<p>Discount Type: <%= @discount.discount_type %></p>
<%= f.label :discount_amount %>
<%= f.number_field :discount_amount %>
<%= f.label :quantity %>
<%= f.number_field :quantity %>
<%= f.submit %>
<% end %>
